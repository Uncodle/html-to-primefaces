{
    "name": "html2primeface",
    "displayName": "html2PrimeFace",
    "description": "Convert html elements into primeface components. This is a simple extension with the purpose to resolve a problem inside a team.",
    "version": "1.0.2",
    "publisher": "uncodle",
    "repository": {
        "type": "git",
        "url": "https://github.com/Uncodle/vscode-html2primeface"
    },
    "icon": "assets/images/logo.png",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.html2PrimefacesConverter"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.html2PrimefacesConverter",
                "title": "html2PrimefacesConverter"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {}
}
