{
    "name": "html-to-primefaces",
    "displayName": "htmlToPrimeFaces",
    "description": "Convert html elemnts to primefaces components. A challenge by: Santander Team",
    "version": "1.0.0",
    "publisher": "uncodle",
    "repository": { 
        "type": "git",
        "url": "https://github.com/Uncodle/vscode-html-to-primefaces"
    },
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.htmlToPrimefacesConverter"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.htmlToPrimefacesConverter",
                "title": "htmlToPrimefacesConverter"
            }
        ]
    },
    "scripts": {
        "fs-extra-install": "npm install fs-extra",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "fs-extra": "^7.0.1"
    }
}
